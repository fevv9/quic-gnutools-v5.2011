#!/bin/bash

set -x
env

cat > conftest.c <<EOF
int main() {
return __MINGW32__;
; return 0; }
EOF

${CC} conftest.c -c 

if test "$?" == "0" ; then
    rm conftest.o conftest.c
    `dirname $0`/win/qdsp6_win_native_configure
else
    rm conftest.c
    `dirname $0`/unix/configure --disable-shared --enable-static
fi

